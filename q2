import java.util.*;
public class acad{
	public static void main(String [] args){
		Scanner sc=new Scanner(System.in);
		int a,b,c;
		System.out.println("Enter the value of a:");
		a =sc.nextInt();
		System.out.println("Enter the value of b:");
		b=sc.nextInt();
		c=a+b;
		System.out.println("The sum of two digits is: "+c);
	
}}
